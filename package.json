{
  "name": "checkout-backend",
  "version": "0.0.1",
  "description": "JCPM checkout backend",
  "author": "Bossabox",
  "contributors": [
    "Mateus Macedo dos Anjos",
    "Caio Leonardo Mendes de Sousa",
    "Claudio Alcantara",
    "Cleber Carvalho",
    "Flavio Vinicius Miranda",
    "Hudson Von Zuben",
    "Stefania Silva"
  ],
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "yarn test --config ./jest-unit.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:watch": "yarn test --watch --config ./jest-unit.config.js",
    "test:cov": "yarn test --coverage --config ./jest-unit.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --config ./jest-unit.config.js",
    "test:e2e": "yarn test --detectOpenHandles --config ./jest-e2e.config.js",
    "prepare": "husky install",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "snyk": "snyk test"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^5.0.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^12.3.4",
    "prettier": "^2.3.2",
    "snyk": "^1.861.0",
    "sonarqube-scanner": "^2.8.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "4.5.5"
  }
}
